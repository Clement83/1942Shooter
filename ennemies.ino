// ou mon ennemie vas s'arreter
int posMinEnnemie = 40;
int posMaxEnnemie = 70;


void initEnnemies()
{
   //init state
  for(int i = 0 ; i< NB_MAX_ENNEMIES; ++i) {
    ennemies[i].life = 1;
    ennemies[i].x = gb.display.width() + random(0,10); //first time we add ennemei randomly
    ennemies[i].y = random(0,gb.display.height() - 10);
    ennemies[i].destX = random(posMinEnnemie, posMaxEnnemie);
    ennemies[i].cadence = random(20, 40);
    
    ennemies[i].fire = NB_FRAME_FIRE_ANIM;
    ennemies[i].isRun = false;
  }
}

void updateEnnemies()
{
  for(int i = 0 ; i< NB_MAX_ENNEMIES; ++i) {
   if(ennemies[i].life>0) {
    if (ennemies[i].fire>0) {
      --ennemies[i].fire;
    } else {
    //la method attend un pointeur sur ennemy nous envoyons donc le pointeur et non l objet directement
      actionEnnemy(&ennemies[i]);
    }
   }
  }
}

void drawEnnemies()
{
  gb.display.setColor(GRAY);
  for(int i = 0 ; i< NB_MAX_ENNEMIES; ++i) {
   if(ennemies[i].life>0) {
    if(ennemies[i].fire>0) {
      gb.display.drawImage( ennemies[i].x  , ennemies[i].y , ennemyFire3, -13,10);
      drawMitraille(ennemies[i].x  , ennemies[i].y + 3, soldat.x + 5, soldat.y + 5);
    } else if(ennemies[i].isRun == true){
      gb.display.drawImage( ennemies[i].x  , ennemies[i].y , ennemyRun3,-11,10);
    } else {
      gb.display.drawImage( ennemies[i].x  , ennemies[i].y , ennemyIdl3,-10,10);
    }
   } else {
    gb.display.drawImage( ennemies[i].x  , ennemies[i].y , ennemyDead);
   }
  }
}


void actionEnnemy(Ennemies *ennemy)//nous utilison un pointer sur l objet afin de pouvoir modifier ses valeurs dans une fonction séparé
{
  if(ennemy->x > ennemy->destX) {
    //if(ennemy->vx % gb.frameCount == 0) {
      --ennemy->x; //on avance
    //}
    ennemy->isRun = true;
   
  } else if(ennemy->cptCadence<1){
    ennemy->isRun = false;
    ennemy->cptCadence = ennemy->cadence;
    ennemy->fire = NB_FRAME_FIRE_ANIM;
  } else if (ennemy->cptCadence>0) {
    ennemy->cptCadence--;
  }
}


